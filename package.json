				"redux-logger": "^3.0.6",
				"redux-persist": "^4.0",
				"redux-saga": "^0.16.0",
				"rnpm": "^1.9.0",
				"tslint-react": "^3.5.1"
		},
		"devDependencies": {
				"babel-preset-react-native": "4.0.0",
				"rimraf": "^2.6.2",
				"tslint": "^5.7.0",
				"typescript": "^2.5.3"
		},
		"rnpm": {
				"assets": [
						"src/assets"
				]
		}
